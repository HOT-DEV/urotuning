{% if settings.display_social %}
<ul class="social hidden-xs">
    {% if settings.footer_social_link_4 != blank %}
  <li class="">
    <a href="{{ settings.footer_social_link_4 }}" title="{{ shop.name }} on Instagram" target="blank"><img alt="Follow UroTuning on Instagram" width="61" class="lazyload" align="absmiddle" data-src="{{ 'ig-136m.png' | asset_img_url: '61x36' }}"></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_1 != blank %}
  <li class="">
    <a href="{{ settings.footer_social_link_1 }}" title="{{ shop.name }} on Facebook" target="blank"><span><i class="fa fa-facebook-official fa-2x" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_2 != blank %}
  <li class="">
    <a href="{{ settings.footer_social_link_2 }}" title="{{ shop.name }} on Twitter" target="blank"><span><i class="fa fa-twitter fa-2x" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_3 != blank %}
  <li class="">
    <a href="{{ settings.footer_social_link_3 }}" title="{{ shop.name }} on Google +" target="blank"><span><i class="fa fa-google-plus fa-2x" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_5 != blank %}
  <li class="">
    <a href="{{ settings.footer_social_link_5 }}" title="{{ shop.name }} on Rss" target="blank"><span><i class="fa fa-youtube-play fa-2x" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
</ul>
<ul class="social visible-xs" style="float:none!important;">
  {% if settings.footer_social_link_1 != blank %}
  <li class="" style="display: inline!important;">
    <a href="{{ settings.footer_social_link_1 }}" title="{{ shop.name }} on Facebook" target="blank"  style="display: inline!important; padding: 0 3px 0!important; color: #000!important; "><span><i class="fa fa-facebook-official fa-lg" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_2 != blank %}
