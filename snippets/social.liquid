    <a href="{{ settings.footer_social_link_3 }}" title="{{ shop.name }} on Google +" target="blank"  style="display: inline!important; padding: 0 3px 0!important; color: #000!important;"><span><i class="fa fa-google-plus fa-lg" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_4 != blank %}
  <li class="" style="display: inline!important;">
    <a href="{{ settings.footer_social_link_4 }}" title="{{ shop.name }} on Instagram" target="blank"  style="display: inline!important; padding: 0 3px 0!important; color: #000!important;"><span><i class="fa fa-instagram fa-lg" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_5 != blank %}
  <li class="" style="display: inline!important;">
    <a href="{{ settings.footer_social_link_5 }}" title="{{ shop.name }} on Rss" target="blank"  style="display: inline!important; padding: 0 3px 0!important; color: #000!important;"><span><i class="fa fa-youtube-play fa-lg" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
</ul>
{% endif %}