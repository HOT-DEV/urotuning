    <a href="{{ settings.footer_social_link_2 }}" title="{{ shop.name }} on Twitter" target="blank"  style="display: inline!important; padding: 0 3px 0!important; color: #000!important;"><span><i class="fa fa-twitter fa-lg" aria-hidden="true"></i></span></a>
  </li>
  {% endif %}
  {% if settings.footer_social_link_3 != blank %}
